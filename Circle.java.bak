class Circle
{
    void area(int r)
    {
        final double pi=3.142;
	       double res = pi*r*r;
	       System.out.println(res);
	      
    
    }
    
    public static void main(String[] args)
    {
         
	    Circle s1=new Circle();
	    
	           s1.area(4,5);
    
    
    }


}